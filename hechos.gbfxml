<wi type="G" value="3588,19,"><wi type="G" value="444,20,">hombre</wi></wi>.
<wi type="G" value="1161,23,">Pero</wi>
<wi type="G" value="1487,22,">si</wi>
<wi type="G" value="4151,24,">un espíritu</wi>
