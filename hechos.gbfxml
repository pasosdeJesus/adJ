<wi type="G" value="1722,8,">en</wi>
<wi type="G" value="846,11,">su</wi>
<wi type="G" value="3588,9,"><wi type="G" value="3624,10,">casa</wi></wi>,
<wi type="G" value="2476,12,">que apareció</wi>
<wi type="G" value="2532,13,">y</wi>
<wi type="G" value="846,15,">le</wi>
<wi type="G" value="2036,14,">dijo</wi>
, `<wi type="G" value="649,16,">Envía</wi>
