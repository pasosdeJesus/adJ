<wi type="G" value="1161,2,"/>
<wi type="G" value="846,3,"><wi type="G" value="2980,4,">habiá dicho</wi></wi>
esto,
<wi type="G" value="1096,5,">se levantó</wi>
<wi type="G" value="4714,6,">una discusión</wi>
<wi type="G" value="3588,7,"><wi type="G" value="5330,8,">entre fariseos</wi></wi>
<wi type="G" value="2532,9,">y</wi>
<wi type="G" value="3588,10,"><wi type="G" value="4523,11,">saduceos</wi></wi>,
