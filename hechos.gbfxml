de <wi type="G" value="3588,11,"><wi type="G" value="5330,12,">de los fariseos</wi></wi>
<wi type="G" value="450,6,">se levantaron</wi>,
<wi type="G" value="1264,13,">y discutieron</wi>
<wi type="G" value="3004,14,">diciendo</wi>, «<wi type="G" value="3762,15,">No</wi>
