<wi type="G" value="4571,5,">te</wi>
<wi type="G" value="2980,2,">dirá</wi>
<wi type="G" value="4487,3,">palabras</wi>
<wi type="G" value="4314,4,">por</wi>
<wi type="G" value="1722,6,"/>
