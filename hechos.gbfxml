la <wi type="G" value="4183,1,">gran</wi>
<wi type="G" value="4714,4,">discusión</wi> ,
<wi type="G" value="3588,6,"><wi type="G" value="5506,7,">el oficial al mando</wi></wi>,
<wi type="G" value="2125,5,">previniendo</wi>
que
<wi type="G" value="3588,10,"><wi type="G" value="3972,11,">Pablo</wi></wi>
<wi type="G" value="3361,8,">no</wi>
<wi type="G" value="1288,9,">fuera destrozado en pedazos</wi>
<wi type="G" value="5259,12,">por</wi>
<wi type="G" value="846,13,">ellos</wi>,
<wi type="G" value="2753,14,">ordenó</wi>
a
<wi type="G" value="3588,15,"><wi type="G" value="4753,16,">los soldados</wi></wi>
<wi type="G" value="2597,17,">que bajaran</wi> y
<wi type="G" value="846,19,">lo</wi>
