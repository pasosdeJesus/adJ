<wi type="G" value="2316,3,">Dios</wi> a
<wi type="G" value="846,11,">sus</wi>
<wi type="G" value="3588,9,"><wi type="G" value="1588,10,">escogidos</wi></wi>,
<wi type="G" value="3588,12,">quienes</wi>
<wi type="G" value="994,13,">lloran</wi>
<wi type="G" value="4314,14,">ante</wi>
<wi type="G" value="846,15,">Él</wi>
