<wi type="G" value="5452,18,">plántate</wi>
<wi type="G" value="1722,19,">en</wi>
<wi type="G" value="3588,20,"><wi type="G" value="2281,21,">el océano</wi></wi>´
<wi type="G" value="2532,22,">y</wi>
<wi type="G" value="302,24,"/><wi type="G" value="5213,25,">les</wi>
<wi type="G" value="5219,23,">obedecería</wi>. </t>
</fr>
</sv>
