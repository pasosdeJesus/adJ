que <wi type="G" value="4571,20,">te</wi> <wi type="G" value="2694,19,">lleve</wi> 
<wi type="G" value="4314,21,">ante</wi> 
<wi type="G" value="3588,22,"><wi type="G" value="2923,23,">el juez</wi></wi>, 
<wi type="G" value="2532,24,">y</wi> 
<wi type="G" value="3588,25,"><wi type="G" value="2923,26,">el juez</wi></wi>
<wi type="G" value="4571,27,">te</wi> 
<wi type="G" value="3860,28,">envíe</wi> 
<wi type="G" value="3588,29,"><wi type="G" value="4233,30,">al oficial</wi></wi>, 
