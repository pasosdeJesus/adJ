<wi type="G" value="2250,7,">el día</wi>
<wi type="G" value="3739,6,">en que</wi>
<wi type="G" value="3575,9,">Noe</wi>
<wi type="G" value="1525,8,">entró</wi>
<wi type="G" value="1519,10,">al</wi>
<wi type="G" value="3588,11,"><wi type="G" value="2787,12,">arca</wi></wi>,
