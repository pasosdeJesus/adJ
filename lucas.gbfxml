<wi type="G" value="5315,13,">yo coma</wi>
<wi type="G" value="2532,14,">y</wi>
<wi type="G" value="4095,15,">beba</wi>
<wi type="G" value="2532,16,">y</wi>
<wi type="G" value="3326,17,">después</wi>
