<wi type="G" value="1096,4,">ocurrió</wi>
<wi type="G" value="1722,5,">en</wi>
<wi type="G" value="3588,6,"><wi type="G" value="2250,7,">los días</wi></wi>
<wi type="G" value="3091,8,">de Lot</wi>:
