<wi type="G" value="2064,9,">iba</wi>
<wi type="G" value="4314,10,">a</wi>
<wi type="G" value="846,11,">él</wi>
<wi type="G" value="3004,12,">diciendo</wi>,
`¡<wi type="G" value="1556,13,"><wi type="G" value="3165,14,">Defiéndeme</wi></wi>
<wi type="G" value="575,15,">de</wi>
<wi type="G" value="3450,18,">mi</wi>
