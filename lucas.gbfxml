<wi type="G" value="4095,10,">bebían</wi>,
<wi type="G" value="59,11,">compraban</wi>,
<wi type="G" value="4453,12,">vendían</wi>,
<wi type="G" value="5452,13,">plantaban</wi>,
<wi type="G" value="3618,14,">construían</wi>; </t>
