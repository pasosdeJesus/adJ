<wi type="G" value="1909,8,">en</wi>
<wi type="G" value="1520,10,">una</wi>
<wi type="G" value="2825,9,">cama</wi>,
<wi type="G" value="3588,11,"/><wi type="G" value="1520,12,">uno</wi>
<wi type="G" value="3880,13,">será tomado</wi>,
<wi type="G" value="2532,14,">y</wi>
