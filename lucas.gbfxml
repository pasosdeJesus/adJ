<wi type="G" value="5213,3,">les</wi>
<wi type="G" value="2046,2,">dirán</wi>,
`¡<wi type="G" value="2400,4,">Miren</wi>,
<wi type="G" value="5602,5,">aquí</wi>!´
<wi type="G" value="2228,6,">o</wi>
`¡<wi type="G" value="2400,7,">Miren</wi>,
<wi type="G" value="1563,8,">allá</wi>!´
<wi type="G" value="3361,9,">No</wi>
<wi type="G" value="565,10,">vayan</wi>,
<wi type="G" value="3366,11,">ni</wi> los
<wi type="G" value="1377,12,">sigan</wi>, </t>
</fr>
</sv>
