+		de_AT.ISO8859-1 \
+		de_AT.UTF-8 \
+		de.ISO8859-1 \
+		de.UTF-8 \
+		el.ISO8859-7 \
+		el.UTF-8 \
+		en.ISO8859-1 \
+		en_US.ISO8859-1 \
+		es.ISO8859-1 \
+		es.UTF-8 \
+		es_CL.ISO8859-1 \
+		es_CL.UTF-8 \
+		es_CO.ISO8859-1 \
+		es_CO.UTF-8 \
+		es_PR.ISO8859-1 \
+		es_PR.UTF-8 \
+		es_US.ISO8859-1 \
+		es_US.UTF-8 \
+		et.ISO8859-15 \
+		et.UTF-8 \
+		eu.ISO8859-1 \
+		fi.ISO8859-1 \
+		fi.UTF-8 \
+		fr.ISO8859-1 \
+		fr.UTF-8 \
+		he.UTF-8 \
+		hr.ISO8859-2 \
+		hr.UTF-8 \
+		hu.ISO8859-2 \
+		hu.UTF-8 \
+		hy.ARMSCII-8 \
+		hy.UTF-8 \
+		is.ISO8859-1 \
+		is.UTF-8 \
+		it.ISO8859-1 \
+		it.UTF-8 \
+		ja.UTF-8 \
+		kk.UTF-8 \
+		ko.UTF-8 \
+		lt.ISO8859-4 \
+		lt.ISO8859-13 \
+		lt.UTF-8 \
+		lv.ISO8859-13 \
+		lv.UTF-8 \
+		mn.UTF-8 \
+		nb.ISO8859-1 \
+		nb.UTF-8 \
+		nl.ISO8859-1 \
+		nn.ISO8859-1 \
+		nn.UTF-8 \
+		pl.ISO8859-2 \
+		pl.UTF-8 \
+		pt_BR.ISO8859-1 \
+		pt_BR.UTF-8 \
+		pt.ISO8859-1 \
+		pt.UTF-8 \
+		ro.ISO8859-2 \
+		ro.UTF-8 \
+		ru.CP1251 \
+		ru.CP866 \
+		ru.ISO8859-5 \
+		ru.KOI8-R \
+		ru.UTF-8 \
+		sk.ISO8859-2 \
+		sk.UTF-8 \
+		sl.ISO8859-2 \
+		sl.UTF-8 \
+		sr.ISO8859-2 \
+		sr.ISO8859-5 \
+		sr.UTF-8 \
+		sv.ISO8859-1 \
+		sv.UTF-8 \
+		tr.ISO8859-9 \
+		tr.UTF-8 \
+		uk.CP1251 \
+		uk.ISO8859-5 \
+		uk.KOI8-U \
+		uk.UTF-8 \
+		zh.UTF-8 \
+		zh_TW.UTF-8
+
+LOCALEDIR=      /usr/share/locale
+
+.SUFFIXES: .src .out
+
+.src.out:
+	grep -v '^#' < ${.IMPSRC} > ${.TARGET}
+	@nl=`wc -l ${.TARGET} | sed -e "s/ ${.TARGET}//g;s/  *//g"`; \
+	if (test "$$nl" != "${TIME_SIZE}") then { \
+	echo "Wrong size of ${.TARGET}: $$nl but expected ${TIME_SIZE}"; \
+	exit 1; \
+	} fi;
+
+FILES=		${LOCALES:S/$/.out/}
+FILESNAME=	LC_TIME
+
+all: ${FILES}
+
+.for locale in ${LOCALES}
+FILESDIR_${locale}.out= ${LOCALEDIR}/${locale}
+.endfor
+
+CLEANFILES=	${FILES}
+
+ENCODINGS=	ISO8859-1 ISO8859-2 ISO8859-4 ISO8859-15 \
+		US-ASCII UTF-8
+
+ES_LINKS=	es_CO:es_GT es_CO:es_PE 
+NO_LINKS=	nb:no_NO
+
+eucKR_CP949=	ko
+GB2312_GBK=	zh
+ISO8859-1_ISO8859-1= ${ES_LINKS} ${NO_LINKS} en_US:af_ZA
+ISO8859-1_ISO8859-15= ca da de de en \
+		es es_CL es_CO es_PR es_US eu \
+		fi fr is it la nb nn pt pt_BR \
+		sv ${ES_LINKS} 
+ISO8859-1_US-ASCII= en
+ISO8859-1_UTF-8= en eu nl
+UTF-8_Big5HKSCS= zh_TW:zh_HK
+UTF-8_UTF-8=	${ES_LINKS} ${NO_LINKS} zh_TW:zh_HK
+
+SYMLINKS=
+.for enc1 in ${ENCODINGS}
+.for enc2 in ${ENCODINGS}
+.for lang_terr in ${${enc1}_${enc2}}
+SYMLINKS+=	${LOCALEDIR}/${lang_terr:C/:.*$//}.${enc1}/${FILESNAME} \
+		${LOCALEDIR}/${lang_terr:C/^.*://}.${enc2}
+.endfor
+.endfor
+.endfor
+
+install:
+.for locale in ${LOCALES}
+	install -o ${LOCALEOWN} -g ${LOCALEGRP} -m ${DIRMODE} -d \
+	    ${DESTDIR}${FILESDIR_${locale}.out}
+	install -o ${LOCALEOWN} -g ${LOCALEGRP} -m ${LOCALEMODE} \
+	    ${locale}.out ${DESTDIR}${FILESDIR_${locale}.out}/${FILESNAME}
+.endfor
+.for lnk file in ${SYMLINKS}
+	@l=${DESTDIR}${lnk}; \
+	 t=${DESTDIR}${file}; \
+	 echo $$t -\> $$l; \
+	 rm -f $$t/${FILESNAME}; mkdir -p $$t; ln -s $$l $$t
+.endfor
+
+uninstall:
+.for locale in ${LOCALES}
+	if (test -f ${DESTDIR}${FILESDIR_${locale}.out}/${FILESNAME}) then { \
+	rm -f ${DESTDIR}${FILESDIR_${locale}.out}/${FILESNAME}; \
+	} fi; \
+	if (test -d ${DESTDIR}${FILESDIR_${locale}.out}/) then { \
+	r=`ls ${DESTDIR}${FILESDIR_${locale}.out}/`; \
+	if (test "$$r" = "") then { \
+	rm -rf ${DESTDIR}${FILESDIR_${locale}.out}/; \
+	} fi; \
+	} fi;
+.endfor
+.for lnk file in ${SYMLINKS}
+	@l=${DESTDIR}${lnk}; \
+	t=${DESTDIR}${file}; \
+	echo $$t -\> $$l; \
+	rm -f $$t/${FILESNAME}; \
+	if (test -d $$t) then { \
+	r=`ls $$t/`; \
+	if (test "$$r" = "") then { \
+	rm -rf $$t; \
+	} fi; \
+	} fi;
+.endfor
+
+transform-fromfreebsd:
+.for locale in ${LOCALES}
+	awk '/# Long month names \(without case ending\)/ { long=1 } /.*/ { if (p != "INI" && long == 0) { print p; } if (long > 0) { long++; if (long == 16) { long = 0; } } p =$$0;} BEGIN { p="INI";} END { print p;}' ${locale}.src | \
+	awk '/# md_order/ { md=1 } /.*/ { if (p != "INI" && md == 0) { print p; } if (md > 0) { md++; if (md == 5) { md = 0; } } p =$$0;} BEGIN { p="INI";} END { print p;}' | \
+	awk -f reordena.awk > /tmp/${locale}
+	cp /tmp/${locale} ${locale}.src
+.endfor
+
+.include <bsd.prog.mk>
