+#!/bin/awk
+# Reordena locale de tiempo de FreeBSD --asegurando 83 lÃ­neas antes
+# a adJ
+
+/.*/ {
+	lin=lin+1;
+	al[lin]=$0;
+}
+
+
+BEGIN {
+	lin=0;
+}
+
+END {
+	# Comentarios encabezado
+	for(i=1; i<6; i++) {
+		print al[i];
+	}
+	# Dias abreviados y completos desde comentario con descripcion
+	for(i=36; i<56; i++) {
+		print al[i];
+	}
+	# Meses abreviados y completos desde comentario con descripcion
+	for(i=6; i<35; i++) {
+		print al[i];
+	}
+	# am pm desde comentario antes de descripcion
+	for(i=67; i<75; i++) {
+		print al[i];
+	}
+	# d_t_fmt = c_fmt desde comentario antes de descripcion
+	for(i=63; i<67; i++) {
+		print al[i];
+	}
+	# d_fmt = x_fmt desde comentario antes de descripcion
+	for(i=59; i<63; i++) {
+		print al[i];
+	}
+	# t_fmt = X_fmt desde comentario antes de descripcion
+	for(i=55; i<59; i++) {
+		print al[i];
+	}
+	# t_fmt_ampm = ampm_fmt desde comentario antes de descripcion
+	for(i=79; i<83; i++) {
+		print al[i];
+	}
+	# date_fmt desde comentario antes de descripcion
+	for(i=75; i<79; i++) {
+		print al[i];
+	}
+	# EOF
+	print al[83];
+}
