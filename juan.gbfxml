<wi type="G" value="3101,8,">discípulo</wi>
<wi type="G" value="1831,1,">salieron</wi>,
<wi type="G" value="2532,9,">y</wi>
<wi type="G" value="2064,10,">fueron</wi>
<wi type="G" value="1519,11,">hacia</wi>
