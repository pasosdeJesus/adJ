<wi type="G" value="2443,7,">para</wi>
<wi type="G" value="4160,8,">hacer</wi>
<wi type="G" value="3588,11,"/><wi type="G" value="1699,12,">mi</wi>
<wi type="G" value="3588,9,"><wi type="G" value="2307,10,">voluntad</wi></wi>,
<wi type="G" value="235,13,">sino</wi>
<wi type="G" value="3588,14,"><wi type="G" value="2307,15,">la voluntad</wi></wi>
