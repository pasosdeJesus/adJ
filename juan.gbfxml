<t xml:lang="es">«<wi type="G" value="3756,4,"><wi type="G" value="3762,7,">No</wi></wi>
<wi type="G" value="2192,5,">tendrías</wi>
<wi type="G" value="1849,6,">poder</wi>
<wi type="G" value="2596,8,">contra</wi>
<wi type="G" value="1700,9,">mí</wi>,
<wi type="G" value="1487,10,">si</wi>
<wi type="G" value="3361,11,">no</wi>
<wi type="G" value="4671,13,">te</wi>
<wi type="G" value="2258,12,">hubiera sido</wi> </t>
