father of it.
<t xml:lang="es"><wi type="G" value="5210,1,">Ustedes</wi>
<wi type="G" value="2075,6,">son</wi>
<wi type="G" value="1537,2,">de</wi>
su <wi type="G" value="3962,3,">padre</wi>,
<wi type="G" value="3588,4,"><wi type="G" value="1228,5,">el diablo</wi></wi>,
<wi type="G" value="2532,7,">y</wi>
<wi type="G" value="2309,13,">quieren</wi>
<wi type="G" value="4160,14,">hacer</wi>
<wi type="G" value="3588,8,"><wi type="G" value="1939,9,">los deseos</wi></wi>
<wi type="G" value="5216,12,">de su</wi>
<wi type="G" value="3588,10,"><wi type="G" value="3962,11,">padre</wi></wi>.
<wi type="G" value="1565,15,">Él</wi>
<wi type="G" value="2258,17,">fue</wi>
<wi type="G" value="443,16,">un asesino</wi>
<wi type="G" value="575,18,">desde</wi>
<wi type="G" value="746,19,">el comienzo</wi>,
<wi type="G" value="2532,20,">y</wi>
<wi type="G" value="3756,24,">no</wi>
