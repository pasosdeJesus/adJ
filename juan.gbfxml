<wi type="G" value="4314,4,">donde</wi>
<wi type="G" value="846,5,">Él</wi>,
<wi type="G" value="846,9,">le</wi>
<wi type="G" value="2065,8,">rogaron</wi>
<wi type="G" value="3306,10,">quedarse</wi>
<wi type="G" value="3844,11,">con</wi>
<wi type="G" value="846,12,">ellos</wi>.
<wi type="G" value="2532,13,">Y</wi>
