<wi type="G" value="3756,5,">no</wi>
<wi type="G" value="5607,6,"/><wi type="G" value="4166,7,">el pastor</wi>,
<wi type="G" value="3739,8,">a quien</wi>
<wi type="G" value="3756,9,">no</wi>
