<wi type="G" value="2532,15,">y</wi>
<wi type="G" value="4100,16,">crea</wi>
<wi type="G" value="1519,17,">en</wi>
<wi type="G" value="846,18,">Él</wi>,
<wi type="G" value="2192,19,">tenga</wi>
<wi type="G" value="2222,20,">vida</wi>
<wi type="G" value="166,21,">eterna</wi>;
<wi type="G" value="2532,22,">y</wi>
