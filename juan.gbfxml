<wi type="G" value="1473,5,">Yo</wi>
<wi type="G" value="5213,7,">les</wi>
<wi type="G" value="2036,6,">dije</wi>:
`<wi type="G" value="1401,10,">Un siervo</wi>
<wi type="G" value="3756,8,">no</wi>
<wi type="G" value="2076,9,">es</wi>
<wi type="G" value="3187,11,">más grande que</wi>
<wi type="G" value="846,14,">su</wi>
<wi type="G" value="3588,12,"><wi type="G" value="2962,13,">señor</wi></wi>.´
