<wi type="G" value="5213,4,">les</wi>
<wi type="G" value="3004,3,">digo</wi>,
<wi type="G" value="3588,5,">aquel que</wi>
<wi type="G" value="3361,6,">no</wi>
<wi type="G" value="1525,7,">entre</wi>
<wi type="G" value="1223,8,">por</wi>
<wi type="G" value="3588,9,"><wi type="G" value="2374,10,">la puerta</wi></wi>
<wi type="G" value="1519,11,">al</wi>
<wi type="G" value="3588,12,"><wi type="G" value="833,13,">rebaño</wi></wi>
<wi type="G" value="3588,14,"><wi type="G" value="4263,15,">de ovejas</wi></wi>,
<wi type="G" value="235,16,">sino que</wi>
<wi type="G" value="305,17,">escale</wi>
<wi type="G" value="237,18,">de otra manera</wi>,
<wi type="G" value="1565,19,"/><wi type="G" value="2076,21,">es</wi>
