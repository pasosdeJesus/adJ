<citebib id="LBA"/> dice `he hablado.´</rf></rb>
<t xml:lang="es"> <wi type="G" value="1537,3,">por</wi>
<wi type="G" value="1683,4,">mi cuenta</wi>,
<wi type="G" value="235,7,">sino que</wi>
<wi type="G" value="3962,11,">el Padre</wi>
que <wi type="G" value="3165,10,">me</wi>
<wi type="G" value="3588,8,"/><wi type="G" value="3992,9,">envió</wi>,
