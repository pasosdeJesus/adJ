<t xml:lang="es"><wi type="G" value="2532,1,">Y</wi>
<wi type="G" value="3825,2,">también</wi>
<wi type="G" value="2087,3,">otra</wi>
<wi type="G" value="1124,4,">escritura</wi>
que <wi type="G" value="3004,5,">dice</wi>,
«<wi type="G" value="3700,6,">Mirarán</wi>
<wi type="G" value="1519,7,">al</wi>
<wi type="G" value="3739,8,">que</wi>
<wi type="G" value="1574,9,">traspasaron</wi>.» </t>
</sv></cm>

<cm>
