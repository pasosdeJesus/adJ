<t xml:lang="es"><wi type="G" value="611,1,">Contestaron</wi>
<wi type="G" value="2532,2,">y</wi>
<wi type="G" value="846,4,">le</wi>
<wi type="G" value="2036,3,">dijeron</wi>, «</t>
