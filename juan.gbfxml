<wi type="G" value="1537,23,">de</wi>
<wi type="G" value="3588,24,"><wi type="G" value="3419,25,">la tumba</wi></wi>,
<wi type="G" value="2532,26,">y</wi>
<wi type="G" value="3756,27,">no</wi> </t>
