<wi type="G" value="2064,9,">vamos</wi>
<wi type="G" value="4862,12,"><wi type="G" value="4671,13,">contigo</wi></wi>.»
<wi type="G" value="1831,14,">Salieron</wi>
<wi type="G" value="2532,15,">y</wi>
<wi type="G" value="2117,20,">de inmediato</wi>
<wi type="G" value="305,16,">entraron</wi>
<wi type="G" value="1519,17,">en</wi>
<wi type="G" value="3588,18,"><wi type="G" value="4143,19,">el bote</wi></wi>.
