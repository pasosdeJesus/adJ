<wi type="G" value="1510,7,">soy</wi>
<wi type="G" value="3588,8,"><wi type="G" value="740,9,">el pan</wi></wi>
<wi type="G" value="3588,10,"><wi type="G" value="2222,11,">de vida</wi></wi>.
