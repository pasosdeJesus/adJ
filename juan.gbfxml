<wi type="G" value="3588,6,"><wi type="G" value="4487,7,">palabras</wi></wi> </t>
<rb xml:lang="es"><wi type="G" value="2532,8,">y</wi>
<wi type="G" value="3361,9,">no</wi>
<wi type="G" value="4100,10,">cree</wi><rf><citebib id="LBA"/> dice `y no las
guarda.´</rf></rb>
<t xml:lang="es">, <wi type="G" value="1473,11,">Yo</wi>
<wi type="G" value="3756,12,">no</wi>
<wi type="G" value="846,14,">lo</wi>
<wi type="G" value="2919,13,">juzgo</wi>,
