<wi type="G" value="3588,7,"/><wi type="G" value="2580,8,">Cana</wi>
<wi type="G" value="3588,9,"><wi type="G" value="1056,10,">de Galilea</wi></wi>,
<wi type="G" value="3699,11,">donde</wi>
<wi type="G" value="4160,12,">había convertido</wi>
<wi type="G" value="3588,13,"><wi type="G" value="5204,14,">el agua</wi></wi> en
