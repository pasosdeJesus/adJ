«<wi type="G" value="3754,8,"/><wi type="G" value="2491,9,">Juan</wi>
<wi type="G" value="3762,13,">no</wi>
<wi type="G" value="4160,12,">hizo</wi>
<wi type="G" value="3303,10,"/><wi type="G" value="4592,11,">señales</wi>,
<wi type="G" value="1161,15,">pero</wi>
<wi type="G" value="3956,14,">todo</wi>
<wi type="G" value="3745,16,">lo que</wi>
<wi type="G" value="2491,18,">Juan</wi>
<wi type="G" value="2036,17,">dijo</wi>
