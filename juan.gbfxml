<wi type="G" value="1080,8,">naciste</wi>
<wi type="G" value="3650,9,">por completo</wi>
<wi type="G" value="1722,5,">en</wi>
<wi type="G" value="266,6,">pecado</wi><rf>Redacción de
<citebib id="DieBibel"/></rf></rb>
<t xml:lang="es"> ¿<wi type="G" value="2532,10,">Pero</wi> <wi type="G" value="4771,11,">tu</wi>
