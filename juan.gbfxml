<wi type="G" value="3588,14,"/><wi type="G" value="2453,15,">los judíos</wi>,
<wi type="G" value="2532,16,">y</wi>
<wi type="G" value="846,18,">les</wi>
<wi type="G" value="3004,17,">dijo</wi>,
