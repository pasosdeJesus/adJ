<wi type="G" value="1097,2,">conoce</wi>,
<wi type="G" value="2504,6,">asi</wi>
<wi type="G" value="1097,7,">Yo conozco</wi>
<wi type="G" value="3588,8,"><wi type="G" value="3962,9,">al Padre</wi></wi>.
<wi type="G" value="2532,10,">Y</wi>
<wi type="G" value="5087,14,">Yo entrego</wi>
<wi type="G" value="3450,13,">mi</wi>
