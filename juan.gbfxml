<wi type="G" value="1586,9,">he escogido</wi>.
<wi type="G" value="235,10,">Pero</wi>
<wi type="G" value="2443,11,">para que</wi>
<wi type="G" value="3588,12,"><wi type="G" value="1124,13,">la escritura</wi></wi>
<wi type="G" value="4137,14,">se cumpla</wi>,
