<wi type="G" value="5213,8,">para ustedes</wi>
<wi type="G" value="2443,9,">que</wi>
<wi type="G" value="1473,10,">Yo</wi>
<wi type="G" value="565,11,">me vaya</wi>,
<wi type="G" value="1063,13,">porque</wi>
