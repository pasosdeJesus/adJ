<wi type="G" value="1785,14,">una orden</wi>
<wi type="G" value="5101,16,">que</wi>
<wi type="G" value="2036,17,">debo decir</wi>
<wi type="G" value="2532,18,">y</wi>
<wi type="G" value="5101,19,">que</wi> </t>
