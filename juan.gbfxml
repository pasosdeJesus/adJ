<wi type="G" value="3762,9,"><wi type="G" value="243,10,">nadie más</wi></wi>
<wi type="G" value="4160,11,">hizo</wi>,
<wi type="G" value="3756,13,">no</wi>
<wi type="G" value="2192,14,">tendrían</wi>
<wi type="G" value="266,12,">pecado</wi>.
<wi type="G" value="1161,16,">Pero</wi>
<wi type="G" value="3568,15,">ahora</wi>
