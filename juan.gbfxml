<wi type="G" value="4396,7,">un profeta</wi>
<wi type="G" value="3756,13,">no</wi>
<wi type="G" value="2192,14,">tiene</wi>
<wi type="G" value="5092,12,">honor</wi>
<wi type="G" value="1722,8,">en</wi>
<wi type="G" value="3588,9,"/><wi type="G" value="2398,10,">su propia</wi>
<wi type="G" value="3968,11,">tierra</wi>. </t>
</sv>
