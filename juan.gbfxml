<wi type="G" value="3956,8,">el que</wi>
<wi type="G" value="3588,9,"/><wi type="G" value="4100,10,">crea</wi>
<wi type="G" value="1519,11,">en</wi>
<wi type="G" value="1691,12,">mi</wi>
<wi type="G" value="3361,16,">no</wi>
