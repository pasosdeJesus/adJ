<wi type="G" value="2981,10,">has dicho</wi>; sino
<wi type="G" value="1063,13,">porque</wi>
<wi type="G" value="191,14,">hemos oído</wi>
<wi type="G" value="846,12,">nosotros mismos</wi>,
