<wi type="G" value="5124,2,">Por eso</wi> 
<wi type="G" value="846,5,">les</wi> 
<wi type="G" value="2980,6,">hablo</wi> 
<wi type="G" value="1722,3,">en</wi> 
<wi type="G" value="3850,4,">parábolas</wi>, 
<wi type="G" value="3754,7,">porque</wi> 
<wi type="G" value="991,8,">viendo</wi> 
<wi type="G" value="3756,9,">no</wi> 
<wi type="G" value="991,10,">ven</wi>, 
<wi type="G" value="2532,11,">y</wi>
<wi type="G" value="191,12,">escuchando</wi> 
<wi type="G" value="3756,13,">no</wi> 
<wi type="G" value="191,14,">escuchan</wi> 
<wi type="G" value="3761,15,">ni</wi> 
<wi type="G" value="4920,16,">entienden</wi>. </t>
