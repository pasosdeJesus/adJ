msgstr ""

#. type: TP
#: contracorriente/man1/zshbuiltins.1:431
#, no-wrap
msgid "B<\\&'E<lt>'> (B<NO_SH_GLOB>)"
msgstr ""

#. type: Plain text
#: contracorriente/man1/zshbuiltins.1:434
msgid "Numeric ranges\\&."
msgstr ""

#. type: TP
#: contracorriente/man1/zshbuiltins.1:434
#, no-wrap
msgid "B<\\&'|'> (B<NO_SH_GLOB>)"
msgstr ""

#. type: Plain text
#: contracorriente/man1/zshbuiltins.1:438
msgid ""
"Alternation in grouped patterns, case statements, or KSH_GLOB parenthesised "
"expressions\\&."
msgstr ""

#. type: TP
#: contracorriente/man1/zshbuiltins.1:438
#, no-wrap
msgid "B<\\&'('> (B<NO_SH_GLOB>)"
msgstr ""

#. type: Plain text
#: contracorriente/man1/zshbuiltins.1:445
msgid ""
"Grouping using single parentheses\\&.  Disabling this does not disable the "
"use of parentheses for B<KSH_GLOB> where they are introduced by a special "
"character, nor for glob qualifiers (use `B<setopt NO_BARE_GLOB_QUAL>\\&' to "
"disable glob qualifiers that use parentheses only)\\&."
msgstr ""

#. type: TP
#: contracorriente/man1/zshbuiltins.1:445
#, no-wrap
msgid "B<\\&'~'> (B<EXTENDED_GLOB>)"
msgstr ""

#. type: Plain text
#: contracorriente/man1/zshbuiltins.1:448
msgid "Exclusion in the form I<A>B<~>I<B>\\&."
msgstr ""

#. type: TP
#: contracorriente/man1/zshbuiltins.1:448
#, no-wrap
msgid "B<\\&'^'> (B<EXTENDED_GLOB>)"
msgstr ""

#. type: Plain text
#: contracorriente/man1/zshbuiltins.1:451
