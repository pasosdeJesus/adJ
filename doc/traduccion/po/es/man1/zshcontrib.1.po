msgstr ""

#. type: TP
#: contracorriente/man1/zshcontrib.1:941
#, no-wrap
msgid "Mercurial (B<hg>)"
msgstr ""

#. type: Plain text
#: contracorriente/man1/zshcontrib.1:944
msgid "B<https://www\\&.mercurial-scm\\&.org/>"
msgstr ""

#. type: TP
#: contracorriente/man1/zshcontrib.1:944
#, no-wrap
msgid "Monotone (B<mtn>)"
msgstr ""

#. type: Plain text
#: contracorriente/man1/zshcontrib.1:947
msgid "B<https://monotone\\&.ca/>"
msgstr ""

#. type: TP
#: contracorriente/man1/zshcontrib.1:947
#, no-wrap
msgid "Perforce (B<p4>)"
msgstr ""

#. type: Plain text
#: contracorriente/man1/zshcontrib.1:950
msgid "B<https://www\\&.perforce\\&.com/>"
msgstr ""

#. type: TP
#: contracorriente/man1/zshcontrib.1:950
