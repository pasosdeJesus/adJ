<wi type="G" value="3361,3,">no</wi>
<wi type="G" value="2564,4,">llamen</wi>
<wi type="G" value="5216,5,"/><wi type="G" value="3962,2,">padre</wi> a hombre alguno
<wi type="G" value="1909,6,">sobre</wi>
<wi type="G" value="3588,7,"/>
<wi type="G" value="1093,8,">la tierra</wi>,
<wi type="G" value="1063,10,">pues</wi>
<wi type="G" value="1520,9,">uno</wi>
<wi type="G" value="2076,11,">es</wi>
<wi type="G" value="3588,12,"/>
<wi type="G" value="5216,14,">su</wi>
<wi type="G" value="3962,13,">Padre</wi>,
