<wi type="G" value="3588,12,"/>
<wi type="G" value="3778,14,">esta</wi>
<wi type="G" value="3571,13,">noche</wi>,
<wi type="G" value="1063,16,">pues</wi>
<wi type="G" value="1125,15,">está escrito</wi>,
`<wi type="G" value="3960,17,">Golpearé</wi>
<wi type="G" value="3588,18,"/>
<wi type="G" value="4166,19,">al pastor</wi>
<wi type="G" value="2532,20,">y</wi>
