<wi type="G" value="5213,2,">de ustedes</wi>
<wi type="G" value="1122,3,">escribas</wi>
<wi type="G" value="2532,4,">y</wi>
<wi type="G" value="5330,5,">fariseos</wi>,
<wi type="G" value="5273,6,">hipócritas</wi>!
<wi type="G" value="3754,7,">Porque</wi>
<wi type="G" value="2808,8,">ustedes cierran</wi>
<wi type="G" value="3588,9,"/>
