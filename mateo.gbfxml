<wi type="G" value="2962,7,">al Señor</wi>
<wi type="G" value="3588,8,"/>
<wi type="G" value="4675,10,">tu</wi>
<wi type="G" value="2316,9,">Dios</wi>
<wi type="G" value="1722,11,">con</wi>
<wi type="G" value="3650,12,">todo</wi>
