<wi type="G" value="1208,1,">el segundo</wi>
es
<wi type="G" value="3664,3,">similiar</wi>
<wi type="G" value="846,4,"/>,
<wi type="G" value="3778,5,"/>
`<wi type="G" value="25,6,">Amarás</wi> a
