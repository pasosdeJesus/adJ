<wi type="G" value="3361,12,"/>
<wi type="G" value="4571,13,">te</wi>
<wi type="G" value="533,14,">negaré</wi>.»
<wi type="G" value="2532,16,">Y</wi>
<wi type="G" value="3956,17,">todos</wi>
<wi type="G" value="3588,18,"/>
