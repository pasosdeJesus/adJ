<wi type="G" value="622,7,">perecerán</wi>
<wi type="G" value="460,5,">sin la ley</wi>.
<wi type="G" value="2532,8,">Y</wi>
<wi type="G" value="3745,9,">tantos como</wi>
<wi type="G" value="264,12,">han pecado</wi>
<wi type="G" value="1722,10,">bajo</wi>
<wi type="G" value="3551,11,">la ley</wi>,
<wi type="G" value="2919,15,">serán juzgados</wi>
