<wi type="G" value="846,4,">su</wi>
<wi type="G" value="3588,2,"><wi type="G" value="5207,3,">Hijo</wi></wi>, </t>
<rb xml:lang="es"><wi type="G" value="2424,13,">Jesus</wi>
<wi type="G" value="5547,14,">Cristo</wi>
<wi type="G" value="2257,17,">nuestro</wi>
