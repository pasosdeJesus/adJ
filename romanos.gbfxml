<wi type="G" value="80,6,">hermanos</wi>,
<wi type="G" value="3754,7,">de que</wi>
<wi type="G" value="4178,8,">frecuentemente</wi>
<wi type="G" value="4388,9,">planeé</wi>
<wi type="G" value="2064,10,">ir</wi>
