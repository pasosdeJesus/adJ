<wi type="G" value="3588,6,"><wi type="G" value="2316,7,">Dios</wi></wi>
<wi type="G" value="1223,8,">por</wi>
<wi type="G" value="3588,9,"><wi type="G" value="2288,10,">la muerte</wi></wi>
