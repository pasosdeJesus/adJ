<wi type="G" value="3588,8,"><wi type="G" value="2316,9,">Dios</wi></wi>
<wi type="G" value="4921,1,">expresa</wi>
<wi type="G" value="3588,3,"/><wi type="G" value="1438,4,">su</wi>
<wi type="G" value="26,5,">amor</wi>
<wi type="G" value="1519,6,">hacia</wi>
<wi type="G" value="2248,7,">nosotros</wi>,
<wi type="G" value="3754,10,">en que</wi>
mientras
<wi type="G" value="2089,11,">aún</wi>
<wi type="G" value="2257,14,"><wi type="G" value="5607,13,">eramos</wi></wi>
<wi type="G" value="268,12,">pecadores</wi>,
<wi type="G" value="5547,15,">Cristo</wi>
<wi type="G" value="599,18,">murió</wi>
<wi type="G" value="5228,16,">por</wi>
<wi type="G" value="2257,17,">nosotros</wi>. </t>

</sv></cm><cm>
