<wi type="G" value="5602,24,">aquí</wi>
<wi type="G" value="4314,25,">con</wi>
<wi type="G" value="2248,26,">nosotros</wi>?»
<wi type="G" value="2532,27,">Y</wi>
<wi type="G" value="4624,28,">estaban escandalizados</wi>
<wi type="G" value="1722,29,">con</wi>
