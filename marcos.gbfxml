<wi type="G" value="1096,9,">había ocurrido</wi> 
<wi type="G" value="1909,10,">en</wi> 
<wi type="G" value="846,11,">ella</wi>, 
<wi type="G" value="2064,12,">vino</wi> 
<wi type="G" value="2532,13,">y</wi> 
<wi type="G" value="4363,14,">cayó</wi> frente 
