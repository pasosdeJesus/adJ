<wi type="G" value="3853,2,">ordenó</wi> <wi type="G" value="3367,5,">no</wi>
<wi type="G" value="2443,4,"/>
<wi type="G" value="142,6,">llevar</wi>
<wi type="GC" value="5">cosa alguna</wi>
<wi type="G" value="1519,7,">para</wi>
<wi type="G" value="3598,8,">el viaje</wi>,
<wi type="G" value="1487,9,"><wi type="G" value="3361,10,">excepto</wi></wi>
<wi type="G" value="3440,12,"/>
<wi type="G" value="4464,11,">un bastón</wi>,
<wi type="G" value="3361,13,">pero no</wi>
