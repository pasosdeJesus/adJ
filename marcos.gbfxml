<wi type="G" value="4675,8,">tu</wi> 
<wi type="G" value="3588,6,"><wi type="G" value="4102,7,">fe</wi></wi> 
<wi type="G" value="4571,10,">te</wi> 
<wi type="G" value="4982,9,">ha hecho bien</wi>. 
<wi type="G" value="5217,11,">Ve</wi> 
<wi type="G" value="1519,12,">en</wi>
<wi type="G" value="1515,13,">paz</wi>, 
<wi type="G" value="2532,14,">y</wi> 
<wi type="G" value="2468,15,">se</wi> 
<wi type="G" value="5199,16,">sana</wi> 
<wi type="G" value="575,17,">de</wi> 
