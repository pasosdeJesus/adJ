<wi type="G" value="3588,17,"><wi type="G" value="5590,18,">el alma</wi></wi>
<wi type="G" value="2532,19,">y</wi>
<wi type="G" value="1537,20,">con</wi>
<wi type="G" value="3650,21,">toda</wi>
<wi type="G" value="3588,22,"><wi type="G" value="2479,23,">la fuerza</wi></wi>,
<wi type="G" value="2532,24,">y</wi>
<wi type="G" value="3588,25,"/><wi type="G" value="25,26,">amar</wi>
<wi type="G" value="3588,27,"/><wi type="G" value="4139,28,">al prójimo</wi>
<wi type="G" value="5613,29,">como</wi>
<wi type="G" value="1438,30,">a sí mismo</wi>
<wi type="G" value="2076,32,">son</wi>
