<wi type="G" value="3588,6,"><wi type="G" value="4151,7,">espíritu</wi></wi>
<wi type="G" value="3754,9,">que</wi>
<wi type="G" value="3779,10,">de esa forma</wi>
<wi type="G" value="1260,11,">razonaban</wi>
<wi type="G" value="1722,12,">con</wi>
<wi type="G" value="1438,13,">si mismos</wi>,
<wi type="G" value="846,15,">les</wi>
