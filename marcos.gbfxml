<wi type="G" value="3588,5,"><wi type="G" value="3480,6,">el Nazareno</wi></wi>

<wi type="G" value="756,8,">comenzó</wi> 
<wi type="G" value="2896,9,">a gritar</wi> 
<wi type="G" value="2532,10,">y</wi> 
<wi type="G" value="3004,11,">decía</wi>,  
«¡<wi type="G" value="2424,15,">Jesús</wi>, 
<wi type="G" value="3588,12,"><wi type="G" value="5207,13,">hijo</wi></wi> 
<wi type="G" value="1138,14,">de David</wi> 
<wi type="G" value="1653,16,">ten piedad</wi> 
<wi type="G" value="3165,17,">de mi</wi>!» </t>
</sv>
