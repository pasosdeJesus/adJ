<wi type="G" value="302,3,"/>
que <wi type="G" value="3361,4,">no</wi>
<wi type="G" value="5209,6,">los</wi>
<wi type="G" value="1209,5,">reciban</wi>
<wi type="G" value="3366,7,">ni</wi>
<wi type="G" value="5216,9,">los</wi>
<wi type="G" value="191,8,">escuchen</wi>,
<wi type="G" value="1607,10,">cuando salgan</wi>
<wi type="G" value="1564,11,">de allí</wi>,
<wi type="G" value="1621,12,">sacudan</wi>
<wi type="G" value="3588,13,"><wi type="G" value="5522,14,">el polvo</wi></wi>
