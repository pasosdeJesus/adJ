<wi type="G" value="5213,25,">les</wi>
<wi type="G" value="3004,24,">digo</wi>,
¡<wi type="G" value="2071,27,">será</wi>
<wi type="G" value="414,26,">más tolerable</wi>
<wi type="G" value="4670,28,">para Sodoma</wi>
<wi type="G" value="2228,29,">y</wi>
<wi type="G" value="1116,30,">Gomorra</wi>
<wi type="G" value="1722,31,">en</wi>
<wi type="G" value="2250,32,">el día</wi>
<wi type="G" value="2920,33,">del juicio</wi>
<wi type="G" value="2228,34,">que</wi>
