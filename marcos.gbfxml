<wi type="G" value="1437,25,"/>
<wi type="G" value="2309,26,">desees</wi>
<wi type="G" value="2532,27,">y</wi> yo
<wi type="G" value="4671,29,">te</wi> lo
<wi type="G" value="1325,28,">daré</wi>.» </t>
</sv>
<sv id="Marcos-6-23">
