<wi type="G" value="846,16,">su</wi>
<wi type="G" value="3588,14,"><wi type="G" value="3968,15,">tierra</wi></wi>,
<wi type="G" value="2532,17,"/>
<wi type="G" value="1722,18,">entre</wi>
<wi type="G" value="3588,19,"/>
<wi type="G" value="4773,20,">sus parientes</wi>
<wi type="G" value="2532,21,">y</wi>
<wi type="G" value="1722,22,">en</wi>
<wi type="G" value="846,25,">su propio</wi>
<wi type="G" value="3588,23,"><wi type="G" value="3614,24,">hogar</wi></wi>.» </t>
</fr>
</sv>
