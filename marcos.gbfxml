<wi type="G" value="2532,9,">y</wi>
<wi type="G" value="3588,10,">de</wi>
<wi type="G" value="4873,11,">los invitados</wi>,
<wi type="G" value="3756,12,">no</wi>
<wi type="G" value="2309,13,">quiso</wi>
<wi type="G" value="114,15,"><wi type="G" value="846,14,">negárselo</wi></wi>. </t>
</sv>
