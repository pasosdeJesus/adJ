<wi type="G" value="5213,14,">ustedes</wi> 
<wi type="G" value="2071,15,">será</wi> 
<wi type="G" value="5216,17,">su</wi> 
<wi type="G" value="1249,16,">servidor</wi>. </t>
</fr>
</sv>
<sv id="Marcos-10-44"><fr>
