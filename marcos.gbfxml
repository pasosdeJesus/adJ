<wi type="G" value="2249,2,"><wi type="G" value="191,3,">escuchamos</wi></wi>
<wi type="G" value="3004,5,">decir</wi>,
`<wi type="G" value="3754,6,"/><wi type="G" value="1473,7,"><wi type="G" value="2647,8,">Destruiré</wi></wi>
<wi type="G" value="5126,11,">este</wi>
<wi type="G" value="3588,9,"><wi type="G" value="3485,10,">templo</wi></wi>
<wi type="G" value="3588,12,">que</wi>
<wi type="G" value="5499,13,">está hecho con manos</wi>,
