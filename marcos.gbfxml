<wi type="G" value="2532,8,">y</wi>
<wi type="G" value="3738,9,">danzó</wi>,
<wi type="G" value="2532,10,"/>
<wi type="G" value="700,11,">agradó</wi>
<wi type="G" value="3588,12,"><wi type="G" value="2264,13,">a Herodes</wi></wi>
