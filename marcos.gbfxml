<wi type="G" value="2254,4,">nos</wi>
<wi type="G" value="1125,3,">escribió</wi>,
`<wi type="G" value="3754,5,"/><wi type="G" value="1437,6,">Si</wi>
<wi type="G" value="80,8,">el hermano</wi>
