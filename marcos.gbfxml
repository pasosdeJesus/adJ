<wi type="G" value="1487,8,">sólo</wi>
<wi type="G" value="3361,9,"/>
<wi type="G" value="2007,12,">puso</wi>
<wi type="G" value="3588,13,"><wi type="G" value="5495,14,">sus manos</wi></wi>
<wi type="G" value="3641,10,">sobre unos pocos</wi>
<wi type="G" value="732,11,">enfermos</wi>
y los
<wi type="G" value="2323,15,">sanó</wi>. </t>
</sv>
<sv id="Marcos-6-6">
