<wi type="G" value="2071,4,">será</wi> 
<wi type="G" value="3779,2,">así</wi> 
<wi type="G" value="1722,5,">entre</wi> 
<wi type="G" value="5213,6,">ustedes</wi>, 
<wi type="G" value="235,7,">más</wi>
<wi type="G" value="3739,8,">quien</wi> 
<wi type="G" value="1437,9,"/><wi type="G" value="2309,10,">quiera</wi> 
<wi type="G" value="1096,11,">ser</wi> 
<wi type="G" value="3173,12,">grande</wi>
