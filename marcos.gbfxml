<wi type="G" value="846,7,">los</wi>
<wi type="G" value="756,6,">comenzó</wi>
<wi type="G" value="649,8,">a enviar</wi>
<wi type="G" value="1417,9,">de dos</wi>
<wi type="G" value="1417,10,">en dos</wi>;
<wi type="G" value="2532,11,"/>
<wi type="G" value="846,13,">les</wi>
<wi type="G" value="1325,12,">dio</wi>
<wi type="G" value="1849,14,">autoridad</wi> </t>
<rb xml:lang="es">sobre <wi type="G" value="3588,15,"><wi type="G" value="4151,16,">los espíritus</wi></wi>
